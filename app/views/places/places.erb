<h1 class="top-header">Welcome, <%= current_user.username %></h1>  
 
 <h2>All places:</h2>
      
 <% @places.each do |place| %>
    <div class="container">
      <div class="row-fluid">
        <div class="col-lg-12">
          <div class= "list">
            <h1><%= place.name.capitalize %> </h1>
              <h5>Reason I want to go?</h5> 
              <p><%= place.content %></p>         
              </div>
          </div>
       </div>
    </div>
 <% end %>
    
         